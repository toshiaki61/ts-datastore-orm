{
  "name": "ts-datastore-orm",
  "version": "2.0.9",
  "description": "ts-datastore-orm targets to provide a strong typed and structural Orm feature for Datastore (Firestore in Datastore mode).",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "test": "echo 1",
    "test:emulator:start": "docker compose up -d datastore-emulator",
    "test:emulator:stop": "docker compose down",
    "test:emulator:all": "npm run test:emulator:start && ./scripts/test-setup.sh bash -c 'npm run _test:general && npm run _test:errors && npm run _test:hook && npm run _test:subclass && npm run _test:compositeIndex'",
    "test:emulator:general": "npm run test:emulator:start && ./scripts/test-setup.sh mocha tests/general.test.ts",
    "test:all": "nyc --reporter=json --reporter=text mocha tests/**/*.ts",
    "test:general": "npm run test:emulator:start && ./scripts/test-setup.sh npm run _test:general",
    "test:query": "npm run test:emulator:start && ./scripts/test-setup.sh npm run _test:query",
    "test:performance": "npm run test:emulator:start && ./scripts/test-setup.sh npm run _test:performance",
    "test:compositeIndex": "npm run test:emulator:start && ./scripts/test-setup.sh npm run _test:compositeIndex",
    "test:subclass": "npm run test:emulator:start && ./scripts/test-setup.sh npm run _test:subclass",
    "test:errors": "npm run test:emulator:start && ./scripts/test-setup.sh npm run _test:errors",
    "test:admin": "npm run test:emulator:start && ./scripts/test-setup.sh npm run _test:admin",
    "test:hook": "npm run test:emulator:start && ./scripts/test-setup.sh npm run _test:hook",
    "test:transactionManager": "npm run test:emulator:start && ./scripts/test-setup.sh npm run _test:transactionManager",
    "test:lockManager": "npm run test:emulator:start && ./scripts/test-setup.sh npm run _test:lockManager",
    "test:helper:incrementHelper": "npm run test:emulator:start && ./scripts/test-setup.sh npm run _test:helper:incrementHelper",
    "test:helper:indexResaveHelper": "npm run test:emulator:start && ./scripts/test-setup.sh npm run _test:helper:indexResaveHelper",
    "_test:general": "mocha tests/general.test.ts",
    "_test:query": "mocha tests/query.test.ts",
    "_test:performance": "mocha tests/performance.test.ts",
    "_test:compositeIndex": "mocha tests/compositeIndex.test.ts",
    "_test:subclass": "mocha tests/subclass.test.ts",
    "_test:errors": "mocha tests/errors.test.ts",
    "_test:admin": "mocha tests/admin.test.ts",
    "_test:hook": "mocha tests/hook.test.ts",
    "_test:transactionManager": "mocha tests/transactionManager.test.ts",
    "_test:lockManager": "mocha tests/lockManager.test.ts",
    "_test:helper:incrementHelper": "mocha tests/incrementHelper.test.ts",
    "_test:helper:indexResaveHelper": "mocha tests/indexResaveHelper.test.ts",
    "performance:tsdatastoreorm": "tsc & node ./build/performance/tsdatastoreorm.performance.js",
    "performance:datastore": "tsc & node ./build/performance/datastore.performance.js",
    "--- deploy ---": "",
    "lint": "biome lint src tests",
    "lint:fix": "biome lint --apply src tests",
    "format": "biome format src tests",
    "format:fix": "biome format --write src tests",
    "check": "biome check src tests",
    "check:fix": "biome check --apply src tests",
    "npm:patch": "npm version patch",
    "tsc:build": "tsc --build --clean && tsc"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.2.4",
    "@google-cloud/datastore": "^10.0.1",
    "@types/chai": "^5.2.2",
    "@types/mocha": "^10.0.1",
    "@types/node": "^24.5.2",
    "chai": "^6.0.1",
    "class-validator": "^0.14.2",
    "mocha": "^11.7.2",
    "nyc": "^17.1.0",
    "protobufjs": "^7.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.2"
  },
  "overrides": {
    "protobufjs": "^7.5.4"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/terence410/ts-datastore-orm.git"
  },
  "author": "Terence",
  "keywords": [
    "datastore",
    "google datastore",
    "google firestore",
    "datastore orm",
    "transaction",
    "google cloud",
    "relational db",
    "typescript"
  ],
  "license": "MIT License",
  "packageManager": "pnpm@10.15.1+sha512.34e538c329b5553014ca8e8f4535997f96180a1d0f614339357449935350d924e22f8614682191264ec33d1462ac21561aff97f6bb18065351c162c7e8f6de67"
}
